algoritmo "Enquanto"

var

    Nomes: Vetor[0..4] de caractere
    I: inteiro

inicio
   
    Nomes[0]<-"Joao"
    Nomes[1]<-"Maria"
    Nomes[2]<-"Jose"
    Nomes[3]<-"Ana"
    Nomes[4]<-"Jorje" 
    
    I<-0
    enquanto I < 5  faca
        escreval (Nomes[I])
        I <- I+1
    fimenquanto

fimalgoritmo