algoritmo "Tipos Variaveis"

var
    Modelo : caractere
    Ano: Inteiro
    Valor: Real
    IPVA,SeguroPago: Logico

inicio
    Modelo<-"Gol"
    Ano<- 2010
    Valor<-20500.14
    IPVA<-FALSO
    SeguroPago<-VERDADEIRO
    escreval ( Modelo)
    escreval ( Ano)
    escreval ( Valor)
    escreval ( IPVA)
    escreva ( VERDADEIRO)
fimalgoritmo